          </Menu>
          <Menu fx:id="menuAjuda" mnemonicParsing="false" text="Ajuda">
            <items>
              <MenuItem fx:id="menuSobre" mnemonicParsing="false" onAction="#onClickBtnSobre" text="Sobre" />
            </items>
          </Menu>
